{
  "cli": {
    "version": ">= 14.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "production": {
    "android": {
      "buildType": "app-bundle",
      "gradleCommand": ":app:bundleRelease",
      "env": {
        "EXPO_USE_HERMES": "1",
        "EAS_SKIP_AUTO_FINGERPRINT": "1"
      }
    }
},
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
